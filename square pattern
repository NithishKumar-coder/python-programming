star=int(input())
for i in range(0,star):
    if i==0 or i==(star-1):
        for j in range(0,star):
            print("*",end=" ")
        print("\r")
    else:
        for j in range(0,star):
            if j==0 or j==(star-1):
                print("*",end=" ")
            else:
                print(" ",end=" ")
        print("\r")
